<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="kbe.protectioncivile.myapplicationrecrutement">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name="kbe.protectioncivile.myapplicationrecrutement.User.UserFormDetailActivity"
            android:exported="false" />
        <activity
            android:name="kbe.protectioncivile.myapplicationrecrutement.antennes.CreateAntenne"
            android:exported="false" />
        <activity
            android:name="kbe.protectioncivile.myapplicationrecrutement.InscriptionActivity"
            android:exported="false" />
        <activity
            android:name="kbe.protectioncivile.myapplicationrecrutement.antennes.AntenneActivity"
            android:exported="false" />
        <activity
            android:name="kbe.protectioncivile.myapplicationrecrutement.User.AllUsersActivity"
            android:exported="false" />
        <activity
            android:name="kbe.protectioncivile.myapplicationrecrutement.Candidature.SuiviCandidatureActivity"
            android:exported="false" />
        <activity
            android:name="kbe.protectioncivile.myapplicationrecrutement.Candidature.PostulerActivity"
            android:exported="false" />
        <activity
            android:name="kbe.protectioncivile.myapplicationrecrutement.FormerActivity"
            android:exported="false" />
        <activity
            android:name="kbe.protectioncivile.myapplicationrecrutement.SecourirActivity"
            android:exported="false" />
        <activity
            android:name="kbe.protectioncivile.myapplicationrecrutement.MyProfileActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="kbe.protectioncivile.myapplicationrecrutement.SplashActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="kbe.protectioncivile.myapplicationrecrutement.AiderActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="kbe.protectioncivile.myapplicationrecrutement.LoginActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="kbe.protectioncivile.myapplicationrecrutement.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>